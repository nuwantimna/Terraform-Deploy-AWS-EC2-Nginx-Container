# Example terraform.tfvars
# Copy this file to terraform.tfvars and update values before running Terraform

vpc_cidr_block       = "10.0.0.0/16"
subnet_cidr_block    = "10.0.10.0/24"
availability_zone    = "us-east-1a"
env_prefix           = "dev"
my_ip                = "YOUR_PUBLIC_IP/32"      # e.g. 203.0.113.25/32
instance_type        = "t3.micro"
private_key_location = "C:/path/to/id_rsa"      # adjust for your system
public_key_location  = "C:/path/to/id_rsa.pub"
